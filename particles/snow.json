{
  "detectRetina": true,
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "onClick": {
        "enable": true,
        "mode": "pause"
      },
      "resize": true
    }
  },
  "particles": {
    "color": {
      "value": "#fff"
    },
    "shape": {
      "type": "circle"
    },
    "opacity": {
      "value": {
        "min": 0.1,
        "max": 0.5
      }
    },
    "size": {
      "value": {
        "min": 1,
        "max": 10
      }
    },
    "move": {
      "enable": true,
      "direction": "bottom",
      "straight": true
    },
    "wobble": {
      "enable": true,
      "distance": 5,
      "speed": {
        "min": -1,
        "max": 1
      }
    }
  }
}
